<?xml version="1.0"?>
<config>
    <modules>
        <AddressFinder_AddressFinder>
            <version>1.0.1</version>
        </AddressFinder_AddressFinder>
    </modules>
    <global>
        <blocks>
            <addressfinder>
                <class>AddressFinder_AddressFinder_Block</class>
            </addressfinder>
        </blocks>
        <helpers>
            <addressfinder>
                <class>AddressFinder_AddressFinder_Helper</class>
            </addressfinder>
        </helpers>
    </global>
    <frontend>
        <layout>
            <updates>
                <addressfinder>
                    <file>addressfinder.xml</file>
                </addressfinder>
            </updates>
        </layout>
    </frontend>
    <default>
        <addressfinder>
            <general>
                <enabled>0</enabled>
                <licence_key>ADDRESSFINDER_DEMO_KEY</licence_key>
                <debug_mode>0</debug_mode>
                <widget_options>{}</widget_options>
            </general>
        </addressfinder>
    </default>
</config>
